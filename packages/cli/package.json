{
  "name": "@billingsdk/cli",
  "version": "0.7.0",
  "description": "A command-line tool for managing billing components and framework integration. Built with TypeScript and designed to work seamlessly with React, Next.js, Express.js, and other popular frameworks.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dodopayments/billingsdk.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "billingsdk": "dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch"
  },
  "keywords": [
    "billing",
    "payments",
    "sdk",
    "cli",
    "react",
    "nextjs",
    "express",
    "paypal",
    "dodopayments"
  ],
  "author": "Dodo Payments Inc.",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@paypal/paypal-server-sdk": "^1.1.0",
    "commander": "^14.0.0",
    "find-up": "^7.0.0",
    "ink": "^6.2.3",
    "ink-big-text": "^2.0.0",
    "react": "^19.1.1",
    "standardwebhooks": "^1.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12"
  }
}